

public class Pipe
{
  public int id ;
  public int cost ;
  public int type ;
  public boolean is_good ;
  public float Xpos;
  public float Ypos;
  public float Angle;
  
  


Pipe (int newId, int newCost , int newType , boolean new_is_good, float newXpos , float  newYpos , float newAngle)
{
  id = newId;
  cost = newCost;
  type = newType;
  is_good = new_is_good;
  Xpos = newXpos;
  Ypos = newYpos;
  Angle = newAngle;


}

}